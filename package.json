{
  "name": "libz7",
  "version": "0.1.0",
  "description": "Compression library based on the LZ77 algorithm",
  "main": "dist/libz7.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prebuild": "babel src --out-dir lib --source-maps && babel samples --out-dir samplestarget --source-maps",
    "build": "npm run test",
    "prepackage": "npm run build",
    "package": "webpack --config webpack.config.js"
  },
  "keywords": [
    "lz77",
    "compress",
    "cli"
  ],
  "author": "Lisa Burton",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^3.2.3",
    "bson": "^3.0.2",
    "cli": "^1.0.1",
    "moment": "^2.22.2",
    "tiny-toatie-cache": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.0.0",
    "jest": "^24.0.0",
    "regenerator-runtime": "^0.12.1",
    "table": "^5.2.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
