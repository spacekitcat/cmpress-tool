  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.5.0",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  }
